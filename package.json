{
    "name": "game",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "clean": "find places -type d -name 'out' -exec rm -rf {} + && find places -type f -name 'flamework.build' -exec rm -f {} +",
        "sync:assets": "asphalt sync && npm run fix:assets",
        "sync-studio:assets": "asphalt sync -t studio && npm run fix:assets",
        "fix:assets": "find places/common/src/shared/assets -type f -exec sed -i 's/\\\\\\\\/\\//g' {} +",
        "compile:game": "rbxtsc -p places/game",
        "watch:game": "rbxtsc -w --verbose -p places/game",
        "build:game": "rojo build -o places/game/build.rbxl places/game",
        "sync:game": "rojo serve --verbose places/game",
        "compile:menu": "rbxtsc -p places/menu",
        "watch:menu": "rbxtsc -w --verbose -p places/menu",
        "build:menu": "rojo build -o places/menu/build.rbxl places/menu",
        "sync:menu": "rojo serve --verbose places/menu"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@rbxts/compiler-types": "^3.0.0-types.0",
        "@rbxts/types": "^1.0.843",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-prettier": "^5.2.5",
        "eslint-plugin-roblox-ts": "^0.0.36",
        "patch-package": "^8.0.0",
        "prettier": "^3.5.3",
        "rbxts-transformer-flamework": "^1.2.4",
        "roblox-ts": "^3.0.0",
        "typescript": "=5.5.3"
    },
    "dependencies": {
        "@flamework/core": "^1.2.4",
        "@flamework/networking": "^1.2.4",
        "@rbxts/flamework-binary-serializer": "^0.6.0",
        "@rbxts/jabby": "^0.2.1",
        "@rbxts/jecs": "^0.5.5",
        "@rbxts/react": "^17.2.3",
        "@rbxts/react-roblox": "^17.2.3",
        "@rbxts/services": "^1.5.5",
        "@rbxts/ui-labs": "^2.3.8"
    }
}
